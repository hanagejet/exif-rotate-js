{
  "name": "exif-rotate-js",
  "version": "0.0.11",
  "description": "When you use input file, you can see preview images.",
  "main": "lib/exif-rotate.js",
  "scripts": {
    "dev": "babel src --out-dir lib --watch & webpack --watch",
    "babel": "babel src --out-dir lib",
    "linter": "eslint --ignore-path .gitignore src/",
    "build": "npm run babel && npm run linter",
    "ava": "ava",
    "test": "npm run linter && npm run ava",
    "prepublish": "npm run build"
  },
  "keywords": [
    "exif"
  ],
  "author": "hanagejet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanagejet/exif-rotate-js"
  },
  "dependencies": {
    "exif-js": "^2.1.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.0.0",
    "webpack": "^1.13.2"
  },
  "ava": {
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  }
}
